<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#99D4F8"
    tools:context=".Friend_Activity"
    android:id="@+id/friend_Act">

    <Button
        android:id="@+id/home_page"
        android:layout_width="56dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="326dp"
        android:layout_marginBottom="659dp"
        android:background="@drawable/ic_baseline_home_24"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/restart_game"
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="661dp"
        android:background="@drawable/ic_baseline_change_circle_24"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/playerChoice1"
        android:layout_width="116dp"
        android:layout_height="95dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="277dp"
        android:layout_marginBottom="414dp"
        android:background="#FFFFFF"
        android:padding="5dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#251E1E"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/playerChoice2"
        android:layout_width="116dp"
        android:layout_height="95dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="411dp"
        android:background="#FCFCFC"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#191616"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/rock_select1"
        android:layout_width="54dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="350dp"
        android:layout_marginBottom="533dp"
        android:background="@drawable/rock"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/paper_select1"
        android:layout_width="56dp"
        android:layout_height="57dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="290dp"
        android:layout_marginBottom="531dp"
        android:background="@drawable/p"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/scissor_select1"
        android:layout_width="55dp"
        android:layout_height="57dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="231dp"
        android:layout_marginBottom="531dp"
        android:background="@drawable/csissors"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/winner_tv"
        android:layout_width="130dp"
        android:layout_height="129dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="71dp"
        android:background="#FAF6F6"
        android:padding="7dp"
        android:text="No Winner"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#252424"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/result"
        android:layout_width="122dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="220dp"
        android:background="#FFFFFF"
        android:padding="7dp"
        android:text="Result"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#1E1C1C"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/counter1"
        android:layout_width="90dp"
        android:layout_height="41dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="313dp"
        android:layout_marginBottom="360dp"
        android:background="#FFFFFF"
        android:padding="7dp"
        android:text="Counter1"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#0B0A0A"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/count1"
        android:layout_width="65dp"
        android:layout_height="35dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="243dp"
        android:layout_marginBottom="362dp"
        android:background="#FFFFFF"
        android:padding="5dp"
        android:text="0"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#090808" />

    <TextView
        android:id="@+id/counter2"
        android:layout_width="90dp"
        android:layout_height="41dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="79dp"
        android:layout_marginBottom="355dp"
        android:background="#FFFFFF"
        android:padding="7dp"
        android:text="counter2"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#191717"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/count2"
        android:layout_width="65dp"
        android:layout_height="35dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="359dp"
        android:background="#FFFFFF"
        android:padding="2dp"
        android:text="0"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#1B1919"
        android:textStyle="bold" />

    <Button
        android:id="@+id/rock_select2"
        android:layout_width="55dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="528dp"
        android:background="@drawable/rock"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/paper_select2"
        android:layout_width="52dp"
        android:layout_height="57dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="530dp"
        android:background="@drawable/p"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/scissor_select2"
        android:layout_width="52dp"
        android:layout_height="57dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="531dp"
        android:background="@drawable/csissors"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="SpeakableTextPresentCheck" />


</RelativeLayout>